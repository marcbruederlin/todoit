    "babel-eslint": "^10.1.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.11",
    "husky": "^8.0.0",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
